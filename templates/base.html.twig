<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
    <meta name="author" content="Ondra Votava">
    <meta name="description" content="Ondra Votava website">

    <title>{% block title %}Resume {% endblock %} | Ondra Votava</title>
    <!-- Fav Icon -->
    <link rel="shortcut icon" href="{{ absolute_url('/favicon.ico') }}">
    <!-- Apple Touch Icon -->
    {#<link rel="apple-touch-icon" href="{{ absolute_url('/images/apple-touch-icon.png') }}">#}

    {% block stylesheets %}
    <link href='https://fonts.googleapis.com/css?family=Roboto' rel='stylesheet'>
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link href="/stylesheets/font-awesome.min.css" rel="stylesheet">
    <link href="/stylesheets/ionicons.min.css" rel="stylesheet">
    <link href="/stylesheets/animate.css" rel="stylesheet">
    <link href="/stylesheets/aos.min.css" rel="stylesheet">
    <link href="/stylesheets/bootstrap.css" rel="stylesheet">
    <link href="/stylesheets/materialize.css" rel="stylesheet">
    <link href="/style.css" rel="stylesheet">

    <!-- HTML5 shiv and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
    <script src="/javascript/html5shiv.min.js"></script>
    <script src="/javascript/respond.min.js"></script>
    <![endif]-->
    {% endblock %}
</head>
<body>
{% block body %}
    {% block page_loader %}{% endblock %}

    {% block header %}
        <div id="header" class="shadow">
            {% block menu %} {% include 'parts/menu.html.twig' %}{% endblock %}
            {% block intro %}
                <!-- Name and Status -->
                <div id="intro" class="container">
                    <h1 class="text-center text-capitalize">Ondra Votava</h1>
                    <h4 class="text-center text-capitalize">Senior Web Developer</h4>
                </div>
            {% endblock %}
        </div>
    {% endblock %}
    {% block content %}

    {% endblock %}

    {% block footer %}
        <footer>
            <div class="container">
                <p class="text-center">
                    {# footer text#}
                </p>
            </div>
        </footer>

        <!-- Back To Top Button -->
        <a href="#header" id="btp" class="back-to-top btn-floating waves-effect waves-light btn-large custom-btn">
            <i class="ion-ios-arrow-up"></i>
        </a>
    {% endblock %}
{% endblock %}
{% block scripts %}
    <!-- ================== SCRIPTS ================== -->
    <script src="/javascript/jquery-2.1.3.min.js"></script>
    <script src="/javascript/jquery.waypoints.min.js"></script>
    <script src="/javascript/aos.min.js"></script>
    <script src="/javascript/bootstrap.min.js"></script>
    <script src="/javascript/materialize.min.js"></script>
    <script src="/javascript/retina.min.js"></script>
    <script src="/javascript/custom-script.js"></script>
{% endblock %}
</body>
</html>
